<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.company.app.controller.S1"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="{i18n>title}">
		<content>
			<IconTabBar id="idIconTabBar" select="handleIconTabBarSelect">
				<items>
					<IconTabFilter icon="sap-icon://retail-store" count="1" text="Pick List"/>
